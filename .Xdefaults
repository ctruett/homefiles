xterm*font:-*-codec terminal text-*-*-*-*-15-*-*-*-*-*-*-*
xterm*boldFont:-*-codec terminal text-*-*-*-*-15-*-*-*-*-*-*-*

*metaSendsEscape:true

*depth: 24
*scrollBar: false
*buffered: false
*boldMode: false

! terminal colors ---------------------
*background: #1e1e1e
*foreground: #cccccc
*cursorColor: #D78787
! Black (not tango) + DarkGrey
*color0:  #1e1e1e
*color8:  #3a3a3a
! DarkRed + Red
*color1:  #D78787
*color9:  #D78787
! DarkGreen + Green
*color2:  #AFD787
*color10: #AFD787
! DarkYellow + Yellow
*color3:  #FFFFAF
*color11: #FFFFAF
! DarkBlue + Blue
*color4:  #87AFD7
*color12: #87AFD7
! DarkMangenta + Mangenta
*color5:  #C787D7
*color13: #C787D7
!DarkCyan + Cyan (both not tango)
*color6:  #87D1D7
*color14: #87D1D7
! LightGrey + White
*color7:  #D0D0D0
*color15: #ffffff
